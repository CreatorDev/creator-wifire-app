endchoice	


source "$HARMONY_VERSION_PATH/bsp/pic32mz_ec_pim+bt_audio_dk/config/bsp.hconfig"
source "$HARMONY_VERSION_PATH/bsp/pic32mz_ec_pim+e16/config/bsp.hconfig"
