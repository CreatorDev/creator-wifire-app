ifblock DS60001191

config BSP_DS60001191_COMMON
    set JTAGEN to "OFF"
    set TRCEN to "OFF"
	set ICESEL to "ICS_PGx2" if USE_BSP
	set FNOSC to "SPLL"
	set FSOSCEN to "OFF"
	set IESO to "OFF"
	set POSCMOD to "EC" if USE_BSP
	set FWDTEN to "OFF"
	set FDMTEN to "OFF"
    set FPLLRNG to "RANGE_5_10_MHZ"
    set FPLLICLK to "PLL_POSC" if USE_BSP
    set FPLLICLK to "PLL_FRC" if !USE_BSP
    set FPLLIDIV to "DIV_3" if USE_BSP
    set FPLLIDIV to "DIV_1" if !USE_BSP
    set FPLLMULT to "MUL_50"
    set FPLLODIV to "DIV_2"
    set TSEQ to "0x0000"
    set CSEQ to "0xffff"
    bool
    default y if USE_BSP

choice
    prompt "Select BSP To Use For $DEVICE Device"
    depends on USE_BSP
    default BSP_PIC32MZ_EC_SK
    ---help---
    IDH_HTML_BSP_Board_Support_Packages
    ---endhelp---

	
config BSP_PIC32MZ_EC_PIM_BT_AUDIO_DK
    depends on USE_BSP
    depends on DS60001191
	select BSP_TRIGGER
	select BSP_POSC_24MHz
    select BSP_LED5
    select BSP_LED6
    select BSP_LED7
    select BSP_LED8
    select BSP_LED9
	select BSP_BT_AUDIO_DK_BOARD
    select USE_GFX_CRYSTAL_2_172x220_NEEDED if BSP_GRAPHICS_NEEDED
    select USE_DRV_GFX_OTM2201A_NEEDED if BSP_GRAPHICS_NEEDED
    bool "PIC32MZ EC PIM w/ Bluetooth Audio Development Kit"
    ---help---
    IDH_HTML_BSP_pic32mz_ec_pim_bt_audio_dk
    ---endhelp---
	
config BSP_PIC32MZ_EC_PIM_E16
    depends on USE_BSP
    depends on DS60001191
	select BSP_TRIGGER
	select BSP_POSC_24MHz
    select BSP_LED3
    select BSP_LED4
    select BSP_LED5
    select BSP_LED6
    select BSP_LED7
    select BSP_LED8
    select BSP_LED9
    select BSP_LED10
    bool "PIC32MZ EC PIM w/ Explorer16"
    ---help---
    IDH_HTML_BSP_pic32mz_ec_pim_e16
    ---endhelp---
	
config BSP_PIC32MZ_EC_SK
